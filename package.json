{
  "name": "ez-test-docs",
  "displayName": "ez-test-docs",
  "description": "Ez Test Docs creates hovers that show your test cases for your functions",
  "version": "0.0.3",
  "publisher": "fitzsimonsdevin",
  "author": {
    "name": "fitzsimonsdevin"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/aisflat439/ez-test-docs"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other",
    "Testing"
  ],
  "keywords": [
    "Javascript",
    "Testing",
    "React Testing Library",
    "Documentation",
    "Jest"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@babel/parser": "^7.25.9",
    "@babel/traverse": "^7.25.9",
    "install": "^0.13.0",
    "npm": "^10.9.0"
  }
}
